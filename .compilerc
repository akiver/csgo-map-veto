{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["es2016-node5", "react"],
        "plugins": [
            "transform-class-properties",
            "transform-object-rest-spread"
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": ["es2016-node5", "react"],
        "plugins": [
            "transform-class-properties",
            "transform-object-rest-spread"
        ],
        "sourceMaps": false
      }
    }
  }
}
